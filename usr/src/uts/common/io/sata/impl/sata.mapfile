	ddi_get_child				{ FLAGS = EXTERN };
	ddi_get_next				{ FLAGS = EXTERN };
	ddi_get_next_sibling			{ FLAGS = EXTERN };
	ddi_pathname				{ FLAGS = EXTERN };
	ddi_set_name_addr			{ FLAGS = EXTERN };

	devcnt					{ FLAGS = EXTERN };
