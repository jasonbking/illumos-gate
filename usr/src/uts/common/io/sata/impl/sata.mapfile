$mapfile_version 2

SYMBOL_VERSION ILLUMOSprivate {
    global:
	# DDI Interfaces
	_fini;
	_init;
	_info;

	# SATA HBA Api
	sata_free_dma_resources;
	sata_free_error_retrieval_pkt;
	sata_free_rdwr_pmult_pkt;
	sata_get_error_retrieval_pkt;
	sata_get_rdwr_pmult_pkt;
	sata_hba_attach;
	sata_hba_detach;
	sata_hba_init;
	sata_hba_fini;
	sata_hba_event_notify;
	sata_register_pmult;
	sata_split_model;
	sata_trace_debug;
	sata_vtrace_debug;

