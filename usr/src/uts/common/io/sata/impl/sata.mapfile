	scsi_hba_nodename_compatible_get	{ FLAGS = EXTERN };
	scsi_hba_nodename_compatible_free	{ FLAGS = EXTERN };

	taskq_create				{ FLAGS = EXTERN };
	taskq_destroy				{ FLAGS = EXTERN };
	taskq_dispatch				{ FLAGS = EXTERN };

	thread_create				{ FLAGS = EXTERN };
	thread_exit				{ FLAGS = EXTERN };

    local:
	*;
};
